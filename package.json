{
  "name": "loopback-vue",
  "version": "1.0.0",
  "main": "server/server.js",
  "scripts": {
    "watch-c": "webpack --config webpack/webpack.client.js -w",
    "watch-s": "webpack --config webpack/webpack.server.js -w",
    "build-c": "webpack --config webpack/webpack.client.js",
    "build-s": "webpack --config webpack/webpack.server.js",
    "build": "npm run build-c && npm run build-s",
    "start": "npm run build && node server/server.js"
  },
  "dependencies": {
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "helmet": "^1.3.0",
    "loopback": "^3.0.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^4.0.0",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^1.0.1",
    "vue": "^2.2.1",
    "vue-router": "^2.3.0",
    "vue-server-renderer": "^2.2.1",
    "vuex-router-sync": "^4.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "css-loader": "^0.23.1",
    "vue-loader": "^9.3.1",
    "webpack": "^1.13.1"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "license": "MIT",
  "description": "Loopback, Vue 2.0, vue-router and vuex starter kit for server side rendering with webpack 2.0"
}
